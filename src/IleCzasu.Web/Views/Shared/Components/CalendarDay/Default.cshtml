
@{
    System.Globalization.CultureInfo culture = new System.Globalization.CultureInfo("pl-PL");
    @model IleCzasu.Application.Models.UserEventsModel;

}
@for (int i = 0; i < 24; i++)
{ 
    <div class="user-calendar-hour row">

        @if (i < 10)
        {
            <div class="hour-label">@("0" + i.ToString())</div>
            <div>
                @foreach (var pe in Model.PrivateEvents)
                {
                  <d>@pe.Name</d>  
                }

            </div>
        }
        else
        {
            <div class="hour-label">@i</div>

          }
    </div>
  }  





