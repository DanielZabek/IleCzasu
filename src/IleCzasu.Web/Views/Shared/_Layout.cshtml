            }
            $(window).resize(function () {
                if ($(window).width() > 640) {
                    $(".weather-box").show();
                    $(".calendar-box").show();
                    clearInterval(interv);
                } else {
                    clearInterval(interv);
                    $(".weather-box").hide();
                    $(".calendar-box").show();
                    var i = 0;
                    interv = setInterval(function () {
                        switch (i) {
                            case 0:
                                $(".calendar-box").hide();
                                $(".weather-box").show();
                                i++;
                                break;
                            case 1:
                                $(".weather-box").hide();
                                $(".calendar-box").show();
                                i = 0;
                                break;
                        }
                    }, 5000);

                }

            });
            $(".events-nav").hover(
                function () {
                    $('.dropdown-menu', this).stop().fadeIn("fast")

                },
                function () {
                    $('.dropdown-menu', this).stop().fadeOut("fast");


                });
            $(".userDropdown").hover(
                function () {
                    $('.dropdown-menu', this).stop().fadeIn("fast")

                },
                function () {
                    $('.dropdown-menu', this).stop().fadeOut("fast");


                });
            $(".tools").hover(
                function () {
                    $('.dropdown-menu', this).stop().fadeIn("fast")

                },
                function () {
                    $('.dropdown-menu', this).stop().fadeOut("fast");


                });
            $(".footer").hover(
                function () {
                    $('.footer-content', this).stop().fadeIn("slow")
                    $("#footerButtonUp").hide();
                    $("#footerButtonDown").show();
                },
                function () {
                    $('.footer-content', this).stop().fadeOut("fast");
                    $("#footerButtonDown").hide();
                    $("#footerButtonUp").show();

                });

            $("#footerButtonUp").click(function () {
                $(this).hide();
                $("#footerButtonDown").show();
                $(".footer-content").show();
            });

            $("#footerButtonDown").click(function () {
                $(this).hide();
                $("#footerButtonUp").show();
                $(".footer-content").hide();
            });

            getLocation();

            function getLocation() {
                if (navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(getWeather);

                } else {
                    alert("Twoja przeglądarka nie obsługuje funkcji geolokacji.");
                }

                }

            function getWeather(position) {

                $.getJSON('@Url.Content("~/")' + "PublicEvents/GetWeather", { lat: position.coords.latitude, lon: position.coords.longitude }, function(w) {
                    $("#weather-city").html(w.name);
                    $("#weather-des").html(w.weather[0].description);
                    document.getElementById("weather-icon").src = "/images/pogoda/" + w.weather[0].icon + ".svg";
                    $("#weather-temp").html(Math.round(w.main.temp) + '&#8451');
                    $("#weather-pres").html(w.main.pressure + " hPa");
                    $("#weather-hum").html('<i class="fas fa-tint"></i>  ' + w.main.humidity + "%");

                });
            }

        });
        window.onload = function () {

            var s = Snap("#clockSVG");
            Snap.load("/images/clock.svg", function (f) {
                //Add SVG elements
                second = f.select("#wSec");
                minute = f.select("#wMin");
                hour = f.select("#wHour");
                //Run animation function for the first time
                animateTime();
                //Append SVG to DIV
                s.append(f);

            });

            $("#imieniny").html(WypiszImieniny());
        };
        setInterval(function () {
            var now = new Date();
            var h = now.getHours();
            var m = now.getMinutes();
            var s = now.getSeconds();
            if (h < 10) {
                h = "0" + h;
            }
            if (m < 10) {
                m = "0" + m;
            }
            if (s < 10) {
                s = "0" + s;
            }
            $("#clock").html(h + " : " + m + " : " + s);
        }, 1000);
        function animateTime() {
            //Get the current time
            var timeNow = new Date();
            var hours = timeNow.getHours();
            var minutes = timeNow.getMinutes();
            var seconds = timeNow.getSeconds();
            //Move second needle halfway
            second.transform('r' + (seconds * 6) + ',100,100');
            //Animate the second needle the rest of the way

            //Move minute needle
            minute.transform('r' + (minutes * 6) + ',100,100');
            //Only animate the minute needle when the minute changes
            if (seconds == 0) {
                minute.transform('r' + (minutes * 6) + ',100,100');

            }
            //Allow the hour needle to move accordingly when the minutes change
            hour.transform('r' + ((hours * 30) + (minutes / 2)) + ',100,100');
            //Repeat this entire function every 1 second
            setTimeout("animateTime()", 1000);
        }
