                $('.search-input').on('input', function () {
                    $('.search-results').fadeIn();
                    var input = $(this).val();
                    if (input.length >= 3) {
                        $('.search-results').load("/PublicEvents/GetSearchResults", { input: input.trim()  }, function () {
                        $(this).find('.search-name').each(function () {
                            var name = $(this).html();
                            var rx = RegExp(input, 'ig');
                            name = name.replace(rx, "<mark>" + input + "</mark>");
                            $(this).html(name);
                        });

                        });
                    } else {
                        $('.search-results').html("");

                    }

                });
                $(".search-button").click(function () {
                    $('.searchbox').removeClass('search-mob');
                    $('.search-results-mob').html("");
